    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^19.0.2",
    "moxios": "^0.4.0"
  },
  "engines": {
    "node": ">=4"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
